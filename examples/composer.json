{
  "require": {
    "smrtr/mysql-version-control": "~1.3"
  },
  "extra": {
    "mysql-version-control": {
      "cli": {
        "--versions-path": "db/commits",
        "--provisional-version": "index"
      },
      "env": {
        "staging": {
          "mysql-bin": "/usr/local/mysql/bin/mysql"
        }
      }
    }
  }
}